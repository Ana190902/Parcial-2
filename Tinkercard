#include <Adafruit_NeoPixel.h>

Adafruit_NeoPixel tira = Adafruit_NeoPixel(64,13, NEO_GRB + NEO_KHZ800);
int Red[8][8]={230,232,218,152,84,95,185,241,232,221,115,69,158,120,88,184,235,189,51,82,151,7,20,20,238,225,100,45,120,11,4,66,161,128,36,58,158,50,143,230,76,18,48,144,155,4,143,243,155,81,62,132,128,136,204,235,114,137,106,103,136,235,236,233};
int Green[8][8]={230,232,212,156,128,133,190,237,232,220,86,116,197,167,147,199,236,176,39,175,191,163,207,136,238,223,79,88,137,88,129,130,169,137,96,145,168,80,159,226,101,130,194,178,153,111,179,238,145,79,116,136,129,173,213,234,87,51,84,34,121,233,235,233};
int Blue[8][8]={230,233,211,143,85,95,185,241,232,221,61,30,161,121,88,184,236,177,8,84,151,7,20,20,239,225,79,34,121,11,4,66,161,85,23,55,158,50,143,230,76,19,50,146,155,4,143,243,150,16,34,119,127,136,204,235,92,2,16,0,89,238,236,233};
int R;
int G;
int B;
int pixel=0;

void prueba(){ //Me genera colores aleatorios
for(int f=0;f<8;f++){
  for(int c=0;c<8;c++){
     Red[f][c]=random(0,255);
     Green[f][c]=random(0,255);
     Blue[f][c]=random(0,255);
 
  }
}
}
	

    
void setup()
{
  tira.begin();
  tira.show();
  tira.setBrightness(255);
  //prueba();
  
  for(int i=0; i<8;i++){
    for(int j=0; j<8;j++){
      
      tira.setPixelColor(j+pixel,Red[i][j],Green[i][j],Blue[i][j]);
      }
    pixel=pixel+8;
        }
  tira.show();
}

void loop()
{
  
  
  
    
    }
      


    
